{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "release": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "lint:eslint": {
      "outputs": []
    },
    "lint:prettier": {
      "outputs": []
    },
    "type:check": {
      "outputs": []
    }
  }
}
